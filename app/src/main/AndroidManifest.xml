<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fonoaudiologia.prosodiappfree">

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:extractNativeLibs="false"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prosodiapp">
        <activity
            android:name="com.fonoaudiologia.prosodiappfree.SplashScreen"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.fonoaudiologia.prosodiappfree.MenuActivity"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:exported="true"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="com.fonoaudiologia.prosodiappfree.Tonica"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:exported="true"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.fonoaudiologia.prosodiappfree.Pergunta"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:exported="true"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.fonoaudiologia.prosodiappfree.Ordem"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:exported="true"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.fonoaudiologia.prosodiappfree.Resultados"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:exported="true"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>