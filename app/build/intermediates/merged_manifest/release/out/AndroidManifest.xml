<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fonoaudiologia"
    android:versionCode="4"
    android:versionName="4.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:extractNativeLibs="false"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prosodiapp" >
        <activity
            android:name="com.fonoaudiologia.prosodiapp.MenuActivity"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fonoaudiologia.prosodiapp.Tonica"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.fonoaudiologia.prosodiapp.Pergunta"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.fonoaudiologia.prosodiapp.Ordem"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.fonoaudiologia.prosodiapp.Resultados"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>